{
    "author":"weis",
    "manifest_version":2,
    "name":"product css selector",
    "description":"A css selector for Eric",
    "version":"0.1",
    "browser_action":{
        "default_icon":{
            "16":"icons/icon16.png",
            "48":"icons/icon48.png",
            "128":"icons/icon128.png"
        },
        "default_title":"product css selector helper"
    },
    "permissions":["activeTab","clipboardRead", "http://mm.geekfinancer.com/*","http://192.168.1.254:8000/*","cookies","webRequest"],
    "content_scripts":[
        {
            "matches":[ "\u003Call_urls>" ],
            "css":["css/content.css"],
            "js":["js/cssPath.js", "js/content.js"],
            "all_frames": true
        }
    ],
    "background":{
        "page":"background.html"
    },
    "icons":{
        "16":"icons/icon16.png",
        "48":"icons/icon48.png",
        "128":"icons/icon128.png"
    },
    "web_accessible_resources":["bar.html","js/bar.js","css/bootstrap.min.css", "css/bar.css", "js/background.js"]
}
